{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "spread": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "semi": false,
              "singleQuote": true,
              "printWidth": 200
            }
        ],
        "indent": [
            "warn",
            2
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "never"
        ],
        "func-names": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "no-shadow": 0,
        "no-await-in-loop": 0,
        "no-restricted-syntax": 0,
        "guard-for-in": 0,
        "jsx-a11y/label-has-associated-control": 0
    }
}
